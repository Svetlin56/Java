class Demo
{
    public static void main(String[] args)
    {
        int i,j,val=1;
        int[][] nums=new int[4][];
        
        for(i=0;i< nums.length;i++)
        {
            nums[i]=new int[i+1];
        }
        for(i=0;i< nums.length;i++)
        {
            for(j=0;j<nums[i].length;j++)
            {
                nums[i][j]=val++;
                System.out.print(nums[i][j]+" ");
            }
            System.out.println();
        }
    }
}
